const c: unknown_type = 0;

fun to_percents(v: i32, a: i32, b: i32): i32 {
    if b <= a {
        return NaN;
    }
    if v < a {
        return 0;
    } elif v > b {
        return 100;
    }
    return (v - a) * 100 / (b - a);
}

